#: libgphoto2_port/gphoto2-port-result.c:42
msgid "No error"
msgstr "無錯誤"

#: libgphoto2_port/gphoto2-port-result.c:44
msgid "Unspecified error"
msgstr "未指明的錯誤"

#: libgphoto2_port/gphoto2-port-result.c:46
msgid "I/O problem"
msgstr "I/O 問題"

