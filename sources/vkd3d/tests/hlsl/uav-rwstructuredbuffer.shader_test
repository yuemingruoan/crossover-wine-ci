[require]
shader model >= 6.2
native-16-bit

[uav 2]
stride 6
size (buffer, 2)

0 0 0

[compute shader]
struct foo
{
    half h;
    int16_t i;
    uint16_t u;
};

RWStructuredBuffer<foo> u : register(u2);

    [numthreads(1, 1, 1)]
void main()
{
    struct foo f0 = {11.125, -12, 13};
    struct foo f1 = {12.25, 13, 0xfff2};
    u[0] = f0;
    u[1] = f1;
}

